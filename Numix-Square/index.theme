Context=Status
Type=Fixed

[22/panel]
Size=22
Context=Status
Type=Fixed

[22@2x/panel]
Size=22
Scale=2
Context=Status
Type=Fixed

[24/panel]
Size=24
Context=Status
Type=Fixed

[24@2x/panel]
Size=24
Scale=2
Context=Status
Type=Fixed

[48/apps]
Size=48
